    {
      "name": "1f8d6184",
      "displayName": "Daily Plant Performance Metrics Overview",
      "queryLines": [
        "SELECT\r\n",
        "  plant_id,\r\n",
        "  plant_name,\r\n",
        "  date,\r\n",
        "  energy_mwh,\r\n",
        "  avg_capacity_factor,\r\n",
        "  outage_hours,\r\n",
        "  lost_energy_mwh,\r\n",
        "  loss_of_profit_usd,\r\n",
        "  avg_turbine_efficiency,\r\n",
        "  avg_inflow_m3s\r\n",
        "FROM catalog_final_project.golden.daily_kpis\r\n",
        "ORDER BY date, plant_id;"
      ],
      "catalog": "catalog_final_project",
      "schema": "default"
    },
    {
      "name": "76930b1f",
      "displayName": "Plant Performance Metrics Ranked by Profit Loss",
      "queryLines": [
        "SELECT\r\n",
        "  plant_id,\r\n",
        "  plant_name,\r\n",
        "  energy_mwh_total,\r\n",
        "  avg_capacity_factor_overall,\r\n",
        "  outage_hours_total,\r\n",
        "  lost_energy_mwh_total,\r\n",
        "  loss_of_profit_usd_total\r\n",
        "FROM catalog_final_project.golden.plant_performance_summary\r\n",
        "ORDER BY loss_of_profit_usd_total DESC;"
      ],
      "catalog": "catalog_final_project",
      "schema": "default"
    }
